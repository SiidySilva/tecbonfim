<p id="notice"><%= notice %></p>

<h1>Sobres</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sobres.each do |sobre| %>
      <tr>
        <td><%= sobre.descricao %></td>
        <td><%= link_to 'Show', sobre %></td>
        <td><%= link_to 'Edit', edit_sobre_path(sobre) %></td>
        <td><%= link_to 'Destroy', sobre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sobre', new_sobre_path %>
